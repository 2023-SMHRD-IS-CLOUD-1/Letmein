<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.db.CodiMapper">
	<select id="codiTop" parameterType="com.smhrd.model.CodiDTO" resultType="com.smhrd.model.CodiDTO">
		SELECT * 
		FROM TB_CODI 
		WHERE CODI_CATEGORY = '상의' 
		AND CODI_GENDER = #{codi_gender}

	</select>
	
	
	
</mapper>